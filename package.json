{
  "name": "frontend",
  "version": "2.0.0",
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "npm run build:next && npm run build:server",
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json --sourceMap",
    "build:storybook": "build-storybook",
    "dev": "nodemon",
    "start": "NODE_ENV=production TS_NODE_PROJECT=tsconfig.server.prod.json node -r tsconfig-paths/register dist/server/index.js",
    "storybook": "start-storybook -p 6006 -s ./public",
    "test:eslint": "eslint --ext .js,.jsx,.ts,.tsx hack server src",
    "test:eslint:ci": "eslint --format ./hack/eslint-formatter.js --ext .js,.jsx,.ts,.tsx hack server src",
    "test:eslint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx hack server src",
    "test:jest": "jest",
    "test:jest:ci": "JEST_JUNIT_OUTPUT_DIR=${ARTIFACTS:-.} jest --ci --reporters=default --reporters=jest-junit",
    "test:lint:ci": "npm run test:eslint:ci && npm run test:typescript",
    "test:typescript": "tsc --noEmit"
  },
  "dependencies": {
    "@algolia/client-search": "^4.4.0",
    "@babel/core": "^7.10.5",
    "@improbable-eng/grpc-web": "^0.13.0",
    "@improbable-eng/grpc-web-node-http-transport": "^0.13.0",
    "@n8tb1t/use-scroll-position": "^1.0.43",
    "@next/bundle-analyzer": "^9.5.2",
    "@sentry/browser": "^5.19.2",
    "@sentry/integrations": "^5.19.2",
    "@sentry/node": "^5.19.2",
    "@sentry/webpack-plugin": "^1.12.1",
    "@types/express": "^4.17.7",
    "@types/google-protobuf": "^3.7.2",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.43",
    "@types/yup": "^0.29.3",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "@zeit/next-stylus": "^1.0.1",
    "algoliasearch": "^4.4.0",
    "autoprefixer": "^9.8.5",
    "babel-loader": "^8.1.0",
    "cacheable-response": "^2.1.0",
    "clsx": "^1.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "express": "^4.17.1",
    "formik": "^2.1.5",
    "google-protobuf": "^3.12.2",
    "http-graceful-shutdown": "^2.3.2",
    "i18next": "^19.6.3",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.20",
    "next": "^9.5.2",
    "next-i18next": "^4.5.0",
    "poststylus": "^1.0.0",
    "promise-retry": "^2.0.1",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-cookie": "^4.0.3",
    "react-dom": "16.13.1",
    "react-i18next": "^11.7.0",
    "react-overlays": "^4.1.0",
    "react-transition-group": "^4.4.1",
    "replacestream": "^4.0.3",
    "rupture": "^0.7.1",
    "stylus": "^0.54.8",
    "stylus-import-if-exist": "^1.1.0",
    "swr": "^0.2.3",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.1-rc",
    "universal-cookie": "^4.0.3",
    "webpack": "^4.43.0",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@sentry/types": "^5.19.2",
    "@storybook/addon-actions": "^6.0.10",
    "@storybook/addon-links": "^6.0.10",
    "@storybook/addon-toolbars": "^6.0.10",
    "@storybook/addons": "^6.0.10",
    "@storybook/react": "^6.0.10",
    "@svgr/cli": "^5.4.0",
    "@types/jest": "^26.0.7",
    "@types/promise-retry": "^1.1.3",
    "@types/react-transition-group": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-custom-alias": "^1.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.8",
