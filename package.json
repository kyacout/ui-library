    "jest-junit": "^11.0.1",
    "jest-localstorage-mock": "^2.4.3",
    "nodemon": "^2.0.4",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^8.0.1",
    "typescript-plugin-css-modules": "^2.4.0"
  },
  "jest-junit": {
    "outputName": "junit_jest.xml",
    "suiteNameTemplate": "{filename}",
    "titleTemplate": "{classname} > {title}"
  }
}
